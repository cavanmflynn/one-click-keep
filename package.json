{
  "name": "one-click-keep",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --mode production",
    "lint": "eslint -c ./config/.eslintrc.js '{src,test}/**/*.ts' --fix",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "format": "prettier --config ./config/prettier.config.js 'src/**/*.{ts,js,scss,html}' --write",
    "start": "vue-cli-service serve --mode development",
    "test:unit": "vue-cli-service test:unit 'test/unit/*.test.ts'"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "ant-design-vue": "^1.6.5",
    "axios": "^0.19.2",
    "bitcoin-core": "^3.0.0",
    "dayjs": "^1.8.20",
    "detect-port": "^1.3.0",
    "docker-compose": "^0.23.5",
    "dockerode": "^3.2.1",
    "electron-log": "^4.2.4",
    "ethers": "^5.0.14",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.20",
    "message-map": "^0.7.2",
    "shell-env": "^3.0.0",
    "strip-ansi": "^6.0.0",
    "typedi": "^0.8.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.0",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.9",
    "@types/detect-port": "^1.3.0",
    "@types/dockerode": "^2.5.34",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-mocha": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "chai": "^4.2.0",
    "electron": "^10.1.2",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "glob-all": "^3.1.0",
    "husky": "^4.2.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.8",
    "node-sass": "^4.13.1",
    "prettier": "^2.1.2",
    "sass-loader": "^8.0.2",
    "typescript": "^3.7.5",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.4",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11",
    "vue-template-loader": "^1.0.0",
    "vuex-module-decorators": "^0.16.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "yarn lint"
    ],
    "*.{ts,js,html,css}": [
      "yarn format"
    ]
  },
  "main": "background.js"
}
